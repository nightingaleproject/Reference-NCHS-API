{
  "AppSettings": {
    "AckAndIJEConversion": false,
    "BFDREnabled" : true,
    "SAMS" : "https://apigw.cdc.gov/OSELS/NCHS/NVSSFHIRAPI",
    "STEVE" : "https://ingress.devfhir.steve.naphsis.us",
    "PageCount" : 100,
    "MaxPayloadSize" : 10000000
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.ApplicationInsights", "Serilog.Sinks.File"
    ],
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "File", "Args": { "path": "Logs/log.txt", "rollingInterval": "Day"} }
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information",
        "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Debug"
      }
    }
  },
  "ConnectionStrings": {
    "NVSSMessagingDatabase": "Server=localhost;Database=nvssmessaging;User=sa;Password=yourStrong$Password;"
  }
}
